<meta xmlns="http://exist-db.org/xquery/repo">
    <description>Logar: Administrative Pages</description>
    <author>Winona Salesky</author>
    <website>http://logarandes.org/</website>
    <status>beta</status>
    <license>GNU-LGPL</license>
    <copyright>true</copyright>
    <type>application</type>
    <target>logar-admin</target>
    <prepare>pre-install.xql</prepare>
    <finish>post-install.xql</finish>
    <config>
        <!-- App title -->
        <title>Logar: Administrative Pages</title>
        <!-- App description -->
        <description>A collaborative research project publishling online reference works concerning the
            culture, history, and literature of Syriac communities from antiquity to the present.</description>
        <!-- Contact email -->
        <contact>info@syriaca.org</contact>
        <!-- Logo name (path from app root) -->
        <logo></logo>
        <!-- Web Site URL -->
        <url>http://logarandes.org/</url>
        <!-- Root of app for building dynamic links. Default is eXist app root -->
        <nav-base>/exist/apps/logar-admin</nav-base>
        <!-- Root for public view of records, allows code sharing accross admin and base app -->
        <public-view>/exist/apps/logar</public-view>
        <!-- eXist app root for app deployment-->
        <app-root>logar-admin</app-root>
        <!-- eXist data app root for gazetteer data -->
        <data-root>logar-data</data-root>
        <comments-root>logar-comments</comments-root>
        <!-- Base URI for identifiers in app data -->
        <base-uri>http://logarandes.org</base-uri>
    </config>
    <permissions user="logar-admin" group="logar" mode="rwxrwxr-x"/>
    
<deployed>2017-09-26T10:13:29.189-04:00</deployed>
</meta>